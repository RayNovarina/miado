<tr>
  <td><%= locals[:channel].slack_channel_name %></td>
  <td><%= locals[:channel].slack_channel_id %></td>
  <td><%= locals[:channel].dm_user_id %></td>
  <td><%= locals[:channel].archived %></td>
  <td><%= locals[:channel].deleted %></td>
  <td><%= locals[:channel].created_at.in_time_zone("Pacific Time (US & Canada)").strftime('%a %B %e, %Y %l:%M%P - PT') %></td>
  <td><%= locals[:channel].updated_at.in_time_zone("Pacific Time (US & Canada)").strftime('%a %B %e, %Y %l:%M%P - PT') %></td>
  <td><%= locals[:tasks].length %></td>
</tr>
